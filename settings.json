{
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": false
  },
  "editor.minimap.enabled": false,
  "files.autoSave": "afterDelay",
  "editor.suggestSelection": "first",
  "editor.renameOnType": true,
  "rust-analyzer.files.watcher": "client",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Community Material Theme Palenight High Contrast",
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.idea/**": true,
    "**/target/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "git.confirmSync": false,
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 14,
  "explorer.openEditors.visible": 0,
  "[python]": {
    "editor.rulers": [105],
    "editor.defaultFormatter": "ms-python.python"
  },
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "editor.renderWhitespace": "all",
  // NOTE: this causes issues with live share
  "files.trimTrailingWhitespace": true,
  "extensions.ignoreRecommendations": false,
  "window.zoomLevel": 0,
  "vetur.useWorkspaceDependencies": true,
  "prettier.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[seed.sql]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.insertSpaces": false
  },
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.checkOnSave.allTargets": false,
  "rust-analyzer.files.exclude": ["target", "node_modules"],
  "rust-client.engine": "rust-analyzer",
  "liveshare.presence": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "javascript",
    "typescript",
    "markdown",
    "vue",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
}
