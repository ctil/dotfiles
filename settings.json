{
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "f", "g"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "q"],
      "commands": ["search.action.focusNextSearchResult"]
    },
    {
      "before": ["[", "q"],
      "commands": ["search.action.focusPreviousSearchResult"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],
  "vim.textwidth": 100,
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.surround": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": true,
    "<C-w>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "extension.vim_ctrl+w",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "x"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    // This works around a bug with undo/redo
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["c-r"],
      "commands": ["redo"]
    }
  ],
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "rust-analyzer.files.watcher": "client",
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy",
    "*html.hbs": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.idea/**": true,
    "**/target/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "git.confirmSync": false,
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 14,
  "explorer.openEditors.visible": 0,
  "[python]": {
    "editor.rulers": [105],
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "extensions.ignoreRecommendations": false,
  "editor.renderWhitespace": "all",

  // Autoformatting stuff that has caused issues with live share in the past
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,

  "eslint.packageManager": "yarn",
  "eslint.validate": ["vue", "typescript"],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[seed.sql]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.insertSpaces": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "[hbs]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "rust-analyzer.files.excludeDirs": [
    "target",
    "node_modules",
    "portals",
    "codepipeline",
    "support",
    "images",
    "scripts"
  ],
  "liveshare.presence": true,
  "search.exclude": {
    "libraries/backend-protos/**": false
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.linting.mypyEnabled": true,
  "rust-analyzer.diagnostics.experimental.enable": false,
  // https://github.com/rust-lang/rust-analyzer/issues/14259
  "rust-analyzer.diagnostics.disabled": ["unresolved-method"],
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "workbench.colorTheme": "Gruvbox Dark (Medium)",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "githubPullRequests.pullBranch": "never",
  "editor.lineNumbers": "relative",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.stickyScroll.enabled": true,
  "vim.highlightedyank.enable": true,
  "vsnotes.defaultNotePath": "~/notes",
  "vsnotes.defaultNoteTitle": "{title}.{ext}",
  "playwright.reuseBrowser": false,
  "debug.allowBreakpointsEverywhere": true,
  "editor.inlineSuggest.enabled": true,
  "editor.autoClosingQuotes": "never",
  "html.autoClosingTags": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "cSpell.diagnosticLevel": "Hint",
  "rust-analyzer.check.command": "check",
  "rust-analyzer.completion.limit": 15,
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.checkOnSave": false
}
