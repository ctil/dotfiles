{
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "extension.vim_ctrl+w",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "R"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.rename",
          "args": []
        }
      ]
    },
    {
      "before": ["]", "h"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.editor.nextChange",
          "args": []
        }
      ]
    },
    {
      "before": ["[", "h"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.editor.previousChange",
          "args": []
        }
      ]
    },
    {
      "before": ["K"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.showHover",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "x"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    // This works around a bug with undo/redo
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["c-r"],
      "commands": ["redo"]
    }
    {
      "before": ["<leader>", "l", "a"],
      "commands": ["problems.action.showQuickFixes"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["find-it-faster.findWithinFiles"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "<space>"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "R"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "lr"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["<C-6>"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "s"],
      "commands": ["search.action.focusNextSearchResult", "<Esc>"]
    },
    {
      "before": ["[", "s"],
      "commands": ["search.action.focusPreviousSearchResult", "<Esc>"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "s"],
      "commands": ["search.action.focusNextSearchResult", "<Esc>"]
    },
    {
      "before": ["[", "s"],
      "commands": ["search.action.focusPreviousSearchResult", "<Esc>"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditorInGroup"]
    }
  ],
  "vim.textwidth": 100,
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.surround": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": true,
    "<C-w>": true
  },
  "vim.highlightedyank.enable": true,
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "files.hotExit": "off",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "rust-analyzer.files.watcher": "client",
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy",
    "*html.hbs": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.idea/**": true,
    "**/target/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "git.confirmSync": false,
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 14,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": false,
  "explorer.openEditors.visible": 1,
  "[python]": {
    "editor.rulers": [105],
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "extensions.ignoreRecommendations": false,
  "editor.renderWhitespace": "all",
  // Autoformatting stuff that has caused issues with live share in the past
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "eslint.validate": ["vue", "typescript"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[seed.sql]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.insertSpaces": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "[hbs]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "rust-analyzer.files.excludeDirs": [
    "target",
    "node_modules",
    "portals",
    "codepipeline",
    "support",
    "images",
    "scripts"
  ],
  "liveshare.presence": true,
  "search.exclude": {
    "libraries/backend-protos/**": false
  },
  "[mjml]": {
    "editor.defaultFormatter": "mjmlio.vscode-mjml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.linting.mypyEnabled": true,
  "rust-analyzer.diagnostics.experimental.enable": false,
  // https://github.com/rust-lang/rust-analyzer/issues/14259
  "rust-analyzer.diagnostics.disabled": ["unresolved-method"],
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "githubPullRequests.pullBranch": "never",
  "editor.lineNumbers": "relative",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.stickyScroll.enabled": true,
  "vsnotes.defaultNotePath": "~/notes",
  "vsnotes.defaultNoteTitle": "{title}.{ext}",
  "playwright.reuseBrowser": false,
  "debug.allowBreakpointsEverywhere": true,
  "editor.inlineSuggest.enabled": true,
  "editor.autoClosingQuotes": "never",
  "html.autoClosingTags": false,
  "cSpell.diagnosticLevel": "Hint",
  "rust-analyzer.completion.limit": 15,
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.checkOnSave": false,
  "rust-analyzer.cargo.extraArgs": ["--target-dir", "./rust-analyzer-target"],
  "lldb.suppressUpdateNotifications": true,
  "find-it-faster.general.useTerminalInEditor": true,
  "find-it-faster.general.killTerminalAfterUse": true,
  "workbench.colorTheme": "Atom One Dark",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "terminal.integrated.fontFamily": "monospace",
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.edits.showCollapsed": false
}
