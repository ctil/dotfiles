{
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": false,
    "<C-w>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "extension.vim_ctrl+w",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    }
  ],
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "rust-analyzer.files.watcher": "client",
  "workbench.colorTheme": "Solarized Light",
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy",
    "*html.hbs": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.idea/**": true,
    "**/target/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "git.confirmSync": false,
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 14,
  "explorer.openEditors.visible": 0,
  "[python]": {
    "editor.rulers": [105],
    "editor.defaultFormatter": "ms-python.python"
  },
  "extensions.ignoreRecommendations": false,
  "editor.renderWhitespace": "all",

  // Autoformatting stuff that has caused issues with live share in the past
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,

  "vetur.useWorkspaceDependencies": true,
  "vetur.dev.vlsPath": "node_modules/.bin/vls",
  "vetur.validation.templateProps": true,
  "eslint.packageManager": "yarn",
  "eslint.validate": ["vue", "typescript"],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[seed.sql]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.insertSpaces": false
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "[hbs]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.files.excludeDirs": [
    "target",
    "node_modules",
    "portals",
    "codepipeline",
    "support"
  ],
  "liveshare.presence": true,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "rust-analyzer.procMacro.enable": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "search.exclude": {
    "libraries/backend-protos/**": true
  },
  "[mjml]": {
    "editor.defaultFormatter": "attilabuti.vscode-mjml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.checkOnSave.allTargets": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "rust-analyzer.checkOnSave.enable": false,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.linting.mypyEnabled": true
}
