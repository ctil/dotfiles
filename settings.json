{
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-k>": false
  },
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "rust-analyzer.files.watcher": "client",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy",
    "*html.hbs": "html"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.idea/**": true,
    "**/target/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "git.confirmSync": false,
  "terminal.integrated.fontSize": 15,
  "editor.fontSize": 14,
  "explorer.openEditors.visible": 0,
  "[python]": {
    "editor.rulers": [105],
    "editor.defaultFormatter": "ms-python.python"
  },
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "editor.renderWhitespace": "all",
  // NOTE: this causes issues with live share
  "files.trimTrailingWhitespace": true,
  "extensions.ignoreRecommendations": false,
  "vetur.useWorkspaceDependencies": true,
  "eslint.packageManager": "yarn",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // NOTE: this causes issues with live share
  "editor.formatOnSave": true,
  "[seed.sql]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.insertSpaces": false
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
  },
  "[hbs]": {
    "editor.formatOnSave": false,
  },
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.checkOnSave.allTargets": true,
  "rust-analyzer.files.excludeDirs": [
    "target",
    "node_modules",
    "portals",
    "codepipeline",
    "support"
  ],
  "rust-client.engine": "rust-analyzer",
  "liveshare.presence": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "javascript",
    "typescript",
    "markdown",
    "vue",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.insertFinalNewline": true,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "rust-analyzer.procMacro.enable": false,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "search.exclude": {
    "**/proto": true,
  },
  "[mjml]": {
    "editor.defaultFormatter": "attilabuti.vscode-mjml"
  },
  "vim.useSystemClipboard": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
}
